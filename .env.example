# =============================================================================
# E-commerce Lakehouse - Environment Variables Template
# =============================================================================
# Copy this file to .env and fill in your own values
# Command: cp .env.example .env
# =============================================================================

# PostgreSQL
POSTGRES_USER=lakehouse
POSTGRES_PASSWORD=your-secure-password-here
POSTGRES_DB=lakehouse

# Airflow
# Generate new Fernet key: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
AIRFLOW_FERNET_KEY=your-fernet-key-here
AIRFLOW_SECRET_KEY=your-secret-key-here
AIRFLOW_ADMIN_USER=admin
AIRFLOW_ADMIN_PASSWORD=your-admin-password-here

# Metabase
MB_DB_USER=lakehouse
MB_DB_PASS=your-secure-password-here
